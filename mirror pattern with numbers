#include<iostream>
using namespace std;
int main()
{
    int n,i,j,l,s;
    cin>>n;
    for(i=-n;i<n-1;i++)
    {
        if(i<0)
        l=-i;
        else
        l=i;
        for(s=0;s<n-l-1;s++)
        {
        cout<<"  ";
        }
        for(j=0;j<n+l-s;j++)
        {
            cout<<j+1<<" ";
        }
        cout<<endl;
    }
    return 0;
}
